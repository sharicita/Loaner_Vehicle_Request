        <order/>
        <published_ref/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
       return;
    }
 
    var gaGetLocation = new GlideAjax('x_1553691_lva.lvaScripts'); //Script include reference

    gaGetLocation.addParam('sysparm_name', 'locationDetail'); //sysparm_name, function in script include
    gaGetLocation.addParam('sysparm_user_sysid', newValue); //requested for user

    gaGetLocation.getXML(setLocation);
    
 }

 function setLocation(response) {
    var answer = response.responseXML.documentElement.getAttribute('answer');
    g_form.setValue('office_location', answer);
 }]]></script>
