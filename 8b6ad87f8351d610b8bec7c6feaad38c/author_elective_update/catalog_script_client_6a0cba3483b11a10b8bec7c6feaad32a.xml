        <name>Date Validation</name>
        <order/>
        <published_ref/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
       return;
    }
 
    // Create variables for the date needed & the return date
    var dateNeeded = g_form.getValue('date_needed');
    var returnDate = g_form.getValue('date_to_return_the_vehicle');

    // If the return date is before date needed, trigger error message
    if (returnDate < dateNeeded) {
        g_form.showFieldMsg('date_to_return_the_vehicle', 'Date to return vehicle should not be before the date needed', 'error', true);
        g_form.clearValue('date_to_return_the_vehicle');
        return false; // Prevent form submission
    }
    return true;
 }
]]></script>
